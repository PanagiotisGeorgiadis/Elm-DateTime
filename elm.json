{
    "type": "package",
    "name": "elm/datetime",
    "summary": "A \"human readable representation\" of date and time built on top of elm/time.",
    "license": "MIT",
    "version": "1.0.0",
    "exposed-modules": [
        "DateTime",
        "Calendar",
        "Clock"
    ],
    "source-directories": [
        "src",
        "tests"
    ],
    "elm-version": "0.19.0 <= v < 0.20.0",
    "dependencies": {
        "elm/core": "1.0.2 <= v < 2.0.0",
        "elm/time": "1.0.0 <= v < 2.0.0"
    },
    "test-dependencies": {
        "elm-explorations/test": "1.2.0 <= v < 2.0.0",
        "elm/random": "1.0.0 <= v < 2.0.0"
    }
}
